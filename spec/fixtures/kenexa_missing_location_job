	<script language="JavaScript">
	function getWidth()
	{
		if (document.layers)
		{
			var w = window.innerWidth;
			var n = w - 180;
			var theimage = "<img src='../../images/pixel.gif' alt='' height=1 width=" + n +"border=1>";
			return theimage;
		}
	}

	//Following Function Added by UmaShankar on 11/29/2001
	function goToSearchPage()
	{
		document.frmSearch.submit();
	}
	function showjob(lngNewSelection)
	{   
	    //''"RDP 294: Added amp; replace.
		document.frmJobDetail.action = "cim_jobdetail.asp?partnerid=25239&siteid=5014&AReq=10150BR&Codes=LOWES";
		document.frmJobDetail.CurSel.value= lngNewSelection;
		document.frmJobDetail.submit();
	}
	function sendToFriend()
	{
		openWindow3('about:blank?sessionid=7BEAADB5042D1C302D4D082DB1A12D27ABEFEE4FEB7D');
		document.frmSendToFriend.submit();
	}

	function saveToJobCart()
	{
		var strTemp;
		var lIdx;
		var lLen;
		var arrSubInfo;

		if (document.frmJobDetail.jobcartcnt.value > 50)
			alert (MSG_cim_jobdetail_JobCart);
		else
		{
			document.frmSaveToCart.chkJobClientIds.value = ReplaceInString(document.frmSaveToCart.chkJobClientIds.value, "%%", "");
			arrSubInfo = document.frmSaveToCart.chkJobClientIds.value.split("|");
			//09-09-02 GTG - Added JobReqLang - Venkat

			if(arrSubInfo.length != 3)
			{
				strTemp =  arrSubInfo[0] + "|" + arrSubInfo[1];
				document.frmSaveToCart.chkJobClientIds.value = strTemp;
			}

			openWindow3('about:blank?sessionid=7BEAADB5042D1C302D4D082DB1A12D27ABEFEE4FEB7D');
			document.frmSaveToCart.submit();
		}
	}

	// Following function added by Sathyanarayanan K on 04/20/2005 for Selectedjobs
	function ApplyJobNow(strLocale)
	{
		var strGQId;
		var iLen, iIndx;
		var arrGQId;
		var arrGQIds;
		var lngNewSelection = 0;
		var lngSiteLocale = "1033"
		strGQId = "90";	// from Querystring

		var lngSelect = 1;
		if  (strGQId == '')
		{
			strGQId= document.frmJobDetail.JobInfo.value;
			arrGQId = (strGQId.substring(2, strGQId.length - 2)).split("%%");
			iLen = arrGQId.length;
			arrGQIds = arrGQId[lngSelect-1].split('|');
			if (arrGQIds[2] != 0)
			{
				if (strLocale == lngSiteLocale)
				{
					openWindow11("/" + strLocale + "/asp/tg/GQLogin.asp?SID=^h7PbXqUV6U5szPZwC098NuM9dSKQrsYYIcGF9ULIaE7kXO_slp_rhc_D_slp_rhc_1OvcvPGnsbnedFMUiu_slp_rhc_7LeqI/Tm_C_R__L_F_7Gcemd0BE0MJFmrHFWDeW9k9O6JudJY=&fjd=true&referer=&gqid="+ arrGQIds[2] + "&jobinfo=__" + arrGQId[lngSelect-1] + "__&applycount=1&type=search_jobdetail");
				}
				else
				{
					document.frmMassApply.action= "/" + strLocale + "/asp/tg/apply.asp?SID=^h7PbXqUV6U5szPZwC098NuM9dSKQrsYYIcGF9ULIaE7kXO_slp_rhc_D_slp_rhc_1OvcvPGnsbnedFMUiu_slp_rhc_7LeqI/Tm_C_R__L_F_7Gcemd0BE0MJFmrHFWDeW9k9O6JudJY=&src=jd&qs=partnerid%3D25239%26siteid%3D5014%26AReq%3D10150BR%26Codes%3DLOWES";
					document.frmMassApply.submit();
				}
			}
			else
			{
				document.frmMassApply.action= "/" + strLocale + "/asp/tg/apply.asp?SID=^h7PbXqUV6U5szPZwC098NuM9dSKQrsYYIcGF9ULIaE7kXO_slp_rhc_D_slp_rhc_1OvcvPGnsbnedFMUiu_slp_rhc_7LeqI/Tm_C_R__L_F_7Gcemd0BE0MJFmrHFWDeW9k9O6JudJY=";
				document.frmMassApply.submit();
			}
		}
		else
		{
	
			if (strGQId != 0)
			{
				if (strLocale == lngSiteLocale)
				{
					openWindow11("/" + strLocale + "/asp/tg/GQLogin.asp?SID=^h7PbXqUV6U5szPZwC098NuM9dSKQrsYYIcGF9ULIaE7kXO_slp_rhc_D_slp_rhc_1OvcvPGnsbnedFMUiu_slp_rhc_7LeqI/Tm_C_R__L_F_7Gcemd0BE0MJFmrHFWDeW9k9O6JudJY=&fjd=true&referer=&gqid=" + document.frmMassApply.GQID.value + "&jobinfo=__1595339|1|90__&applycount=1&type=search_jobdetail");
				}
				else
				{
					document.frmMassApply.action= "/" + strLocale + "/asp/tg/apply.asp?SID=^h7PbXqUV6U5szPZwC098NuM9dSKQrsYYIcGF9ULIaE7kXO_slp_rhc_D_slp_rhc_1OvcvPGnsbnedFMUiu_slp_rhc_7LeqI/Tm_C_R__L_F_7Gcemd0BE0MJFmrHFWDeW9k9O6JudJY=&src=jd&qs=partnerid%3D25239%26siteid%3D5014%26AReq%3D10150BR%26Codes%3DLOWES";
					document.frmMassApply.submit();
				}
     	    }
     	    else
     		{
     			document.frmMassApply.action		= "/" + strLocale + "/asp/tg/Apply.asp?SID=^h7PbXqUV6U5szPZwC098NuM9dSKQrsYYIcGF9ULIaE7kXO_slp_rhc_D_slp_rhc_1OvcvPGnsbnedFMUiu_slp_rhc_7LeqI/Tm_C_R__L_F_7Gcemd0BE0MJFmrHFWDeW9k9O6JudJY=";
				document.frmMassApply.submit();
     		}
     	}
	}
	// End

	function ApplyAsRegularJob()
	{
		document.frmMassApply.target = "_self";
		document.frmMassApply.GQID.value = "0";
		document.frmMassApply.action = "apply.asp?SID=^h7PbXqUV6U5szPZwC098NuM9dSKQrsYYIcGF9ULIaE7kXO_slp_rhc_D_slp_rhc_1OvcvPGnsbnedFMUiu_slp_rhc_7LeqI/Tm_C_R__L_F_7Gcemd0BE0MJFmrHFWDeW9k9O6JudJY=";
		document.frmMassApply.submit();
	}

	// TD:74615 : Added by Sathyanarayanan K on 29.07.2005
	function reload(sParam)
	{
		if (typeof(sParam) != 'undefined') 
		{
			if (sParam == 'ApplyAsRegularJob')
				ApplyAsRegularJob();
			return;
		}
		var sGQId =  "90";
		var arrGQId;
		var iLen = 0;
		if (sGQId == '' )
		{
			sGQId= document.frmJobDetail.JobInfo.value;
			arrGQId = (sGQId.substring(2, sGQId.length - 2)).split("%%");
			iLen = arrGQId.length;
		}
		
		if ((iLen == 1) || (sGQId != ''))
		{
			
		document.frmJobDetail.target = "_self";
		document.frmJobDetail.CurSel.value = "";
		document.frmJobDetail.action = "Logout.asp?SID=^h7PbXqUV6U5szPZwC098NuM9dSKQrsYYIcGF9ULIaE7kXO_slp_rhc_D_slp_rhc_1OvcvPGnsbnedFMUiu_slp_rhc_7LeqI/Tm_C_R__L_F_7Gcemd0BE0MJFmrHFWDeW9k9O6JudJY=&gqclose=yes&crl=yes&Exit=^";
		document.frmJobDetail.submit();
		}
	}

	//Following function Added by UmaShankar on 12/26/2001 for Mass apply from jobdetail
	function ApplyNow()
	{
		// Start of changes TD 	35552 .. 7/22/2003
			var ApplyURL = "";

				if (ApplyURL=="-1" ) ApplyURL= "";
				if (ApplyURL.length>0 )
				{
				
				 window.location.href=ApplyURL; 
				 }
				else
				//In case GTG if the job siteid is different than the current siteid
				//display warning pop-up to confirm site switched to job site
				{
				
				
					
						ApplyJobNow('1033');
					
				}

// End of Changes
	}
	//End Add

